{
    "name": "dormcon-website",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "prestart": "yarn run api-docs",
        "start": "docusaurus start",
        "prebuild": "yarn run api-docs",
        "build": "docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "gen-api-docs": "docusaurus gen-api-docs all",
        "clean-api-docs": "docusaurus clean-api-docs all",
        "api-docs": "yarn run clean-api-docs && yarn run gen-api-docs",
        "format": "prettier --write '**/*.{ts,tsx,js,json,css,md,mdx,yml}'",
        "lint": "prettier --check . && eslint .",
        "check-types": "tsc"
    },
    "engines": {
        "node": ">=18.0"
    },
    "dependencies": {
        "@babel/core": "7.25.7",
        "@babel/helpers": "7.25.7",
        "@docusaurus/core": "3.5.2",
        "@docusaurus/cssnano-preset": "3.5.2",
        "@docusaurus/preset-classic": "3.5.2",
        "@getcanary/docusaurus-theme-search-pagefind": "^1.0.0",
        "@getcanary/web": "^1.0.2",
        "@mdx-js/react": "^3.0.1",
        "chart.js": "^4.4.4",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "docusaurus-plugin-openapi-docs": "^4.1.0",
        "docusaurus-theme-openapi-docs": "^4.1.0",
        "fuse.js": "^7.0.0",
        "lodash": "^4.17.21",
        "react": "^18.3.1",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "^18.3.1",
        "react-json-view-lite": "1.5.0",
        "react-twitter-widgets": "^1.11.0",
        "swr": "^2.2.5"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@docusaurus/eslint-plugin": "3.5.2",
        "@docusaurus/module-type-aliases": "3.5.2",
        "@docusaurus/tsconfig": "3.5.2",
        "@docusaurus/types": "3.5.2",
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.12.0",
        "@types/eslint": "^9.6.1",
        "@types/eslint__eslintrc": "^2.1.2",
        "@types/eslint__js": "^8.42.3",
        "@types/gtag.js": "^0.0.20",
        "@types/lodash": "^4.17.9",
        "@types/react": "^18.3.10",
        "eslint": "^9.12.0",
        "eslint-plugin-jsx-a11y": "^6.10.0",
        "eslint-plugin-react": "^7.37.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "globals": "^15.9.0",
        "prettier": "^3.3.3",
        "typescript": "^5.6.2",
        "typescript-eslint": "^8.7.0"
    },
    "prettier": {
        "tabWidth": 4,
        "proseWrap": "always"
    }
}
